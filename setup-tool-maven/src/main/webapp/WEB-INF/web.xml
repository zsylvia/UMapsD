<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

	<servlet>
		<servlet-name>RdpServlet</servlet-name>
		<servlet-class>edu.umassd.umapsd.servlet.RdpServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>RenameServlet</servlet-name>
		<servlet-class>edu.umassd.umapsd.servlet.RenameServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>GraphUploadServlet</servlet-name>
		<servlet-class>edu.umassd.umapsd.servlet.GraphUploadServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>DictionaryUploadServlet</servlet-name>
		<servlet-class>edu.umassd.umapsd.servlet.DictionaryUploadServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>MainServlet</servlet-name>
		<servlet-class>edu.umassd.umapsd.servlet.MainServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>RdpServlet</servlet-name>
		<url-pattern>/rdp/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RenameServlet</servlet-name>
		<url-pattern>/rename/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GraphUploadServlet</servlet-name>
		<url-pattern>/graphUpload/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DictionaryUploadServlet</servlet-name>
		<url-pattern>/dictionaryUpload/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>MainServlet</servlet-name>
		<url-pattern>/main/*</url-pattern>
	</servlet-mapping>
	
</web-app>